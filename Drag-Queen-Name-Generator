def nacimientodrag(nacimiento):
    ultnum = nacimiento % 10
    if ultnum == 0:
        drag3 = "I'm every woman by Chaka Khan"
    elif ultnum == 1:
        drag3 = "Rabiosa by Shakira"
    elif ultnum == 2:
        drag3 = "Poker face by Lady Gaga"
    elif ultnum == 3:
        drag3 = "Believe by Cher"
    elif ultnum == 4:
        drag3 = "Break free by Ariana Grande"
    elif ultnum == 5:
        drag3 = "Whip my hair by Willow Smith"
    elif ultnum == 6:
        drag3 = "I will survive by Gloria Garner"
    elif ultnum == 7:
        drag3 = "It's raining man by Martha Wash ans Rupaul"
    elif ultnum == 8:
        drag3 = "Shut up and drive by Rihanna"
    else:
        drag3 = "Toxic by Britney Spears"
    return drag3


def mascotadrag():
    mascotas = ["ABCDarius", "AlexPlosivo", "Elvis Cochuelo", "Igor Dito", "Hairy Paw-ter", "Ozzy Pawsbourne", "Kareem Abdul Ja-Bark"]
    numero_random = random.randrange(len(mascotas))
    return mascotas[numero_random]


nombre = input("Nombre: ")
apellido = input("Apellido: ")
nacimiento = int(input("Año en que naciste: "))

print('\n', "Tu nombre de drag queen es: ", nombredrag(nombre), apellidodrag(apellido),'\n', "Canción que te representa:", nacimientodrag(nacimiento), '\n',"Tu mascota drag se llama: ", mascotadrag())
