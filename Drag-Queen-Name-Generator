import random
def nombredrag(nombre):
    n2 = nombre.upper()
    inicial = n2[0]
    if inicial == "A":
        drag1 = "Peaches"
    elif inicial == "B":
        drag1 = "Backroll"
    elif inicial == "C":
        drag1 = "Candy"
    elif inicial == "D":
        drag1 = "Diamond"
    elif inicial == "E":
        drag1 = "Eve"
    elif inicial == "F":
        drag1 = "Fur"
    elif inicial == "G":
        drag1 = "Ginger"
    elif inicial == "H":
        drag1 = "Holly"
    elif inicial == "I":
        drag1 = "Ivy"
    elif inicial == "J":
        drag1 = "Jade"
    elif inicial == "K":
        drag1 = "Koko"
    elif inicial == "L":
        drag1 = "Lemon"
    elif inicial == "M":
        drag1 = "Marshmallow"
    elif inicial == "N":
        drag1 = "Nibbles"
    elif inicial == "O":
        drag1 = "Orgazma"
    elif inicial == "P":
        drag1 = "Pussy"
    elif inicial == "Q":
        drag1 = "Sequence"
    elif inicial == "R":
        drag1 = "Ru"
    elif inicial == "S":
        drag1 = "Stinx"
    elif inicial == "T":
        drag1 = "Tyra"
    elif inicial == "U":
        drag1 = "Uriel"
    elif inicial == "V":
        drag1 = "Venus"
    elif inicial == "W":
        drag1 = "Widow"
    elif inicial == "X":
        drag1 = "Miss"
    elif inicial == "Y":
        drag1 = "Yara"
    else:
        drag1 = "Lady"
    return drag1

